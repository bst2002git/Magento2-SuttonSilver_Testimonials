					<resource id="SuttonSilver_Testimonials::Testimonial_delete" sortOrder="20" title="Delete Testimonial"/>
					<resource id="SuttonSilver_Testimonials::Testimonial_update" sortOrder="30" title="Update Testimonial"/>
